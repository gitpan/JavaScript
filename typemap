OUTPUT

jsval
    {
        SV *orz = sv_newmortal();
        /* Convert js value to perl */
        JSVALToSV(PJS_GetJSContext(cx), NULL, $var, &orz);
        sv_setsv($arg, orz);
        JS_GC(PJS_GetJSContext(cx));
    }
            
