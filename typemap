jsval			        jsval

OUTPUT
jsval
    {
        SV *orz = sv_newmortal();
        /* Convert js value to perl */
        JSVALToSV(cx->cx, NULL, $var, &orz);
        sv_setsv($arg, orz);
        JS_GC(cx->cx);
    }
            
